# OpenAI API Key
OPENAI_API_KEY=

# Other providers
ANTHROPIC_API_KEY=
GROQ_API_KEY=
FIREWORKS_API_KEY=
TOGETHER_API_KEY=
GOOGLE_AI_API_KEY=
GOOGLE_VERTEX_CREDENTIALS=
MISTRAL_API_KEY=
XAI_API_KEY=
NEXT_PUBLIC_SITE_URL=

# Rate limit
RATE_LIMIT_MAX_REQUESTS=
RATE_LIMIT_WINDOW=

# Vercel/Upstash KV (short URLs, rate limiting)
KV_REST_API_URL=
KV_REST_API_TOKEN=

# Supabase (auth)
SUPABASE_URL=
SUPABASE_ANON_KEY=

# PostHog (analytics)
NEXT_PUBLIC_POSTHOG_KEY=
NEXT_PUBLIC_POSTHOG_HOST=

# Solana Configuration
SOLANA_DEVNET_RPC=https://api.devnet.solana.com
SOLANA_MAINNET_RPC=https://api.mainnet-beta.solana.com

# Solana Payer Wallet (JSON array of private key bytes)
# Generate with: solana-keygen new --outfile keypair.json --no-bip39-passphrase
# Then copy the array from the JSON file
SOLANA_PAYER_PRIVATE_KEY=[1,2,3,4,5,...]

# Arweave Configuration
# Generate wallet at: https://arweave.app/wallet
# Download wallet JSON and stringify it
ARWEAVE_WALLET_KEY={"kty":"RSA","n":"...","e":"AQAB",...}

# Next.js Configuration
NEXT_PUBLIC_SOLANA_NETWORK=devnet
